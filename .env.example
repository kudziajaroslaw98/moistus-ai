NEXT_PUBLIC_APP_LOCAL_HREF='http://localhost:3000'
NEXT_PUBLIC_SUPABASE_URL=
NEXT_PUBLIC_SUPABASE_ANON_KEY=
SUPABASE_SERVICE_ROLE=
# Optional overrides for PartyKit JWT verification.
# If left empty, PartyKit derives them from NEXT_PUBLIC_SUPABASE_URL.
SUPABASE_JWKS_URL=
SUPABASE_JWT_ISSUER=
# Optional strict audience check (Supabase user JWTs usually use "authenticated")
SUPABASE_JWT_AUDIENCE=
GOOGLE_GENERATIVE_AI_API_KEY=
OPENAI_API_KEY=

# PartyKit realtime (Yjs transport)
NEXT_PUBLIC_PARTYKIT_URL='127.0.0.1:1999'
NEXT_PUBLIC_PARTYKIT_PARTY='main'
PARTYKIT_BASE_URL='http://127.0.0.1:1999'
PARTYKIT_PARTY_NAME='main'
PARTYKIT_ADMIN_TOKEN=

# Development Mode - Bypass subscription limits for local development
# Set to 'true' to disable node/map/AI suggestion limits
NEXT_PUBLIC_DEV_BYPASS_LIMITS=false

# Feature Flags
# AI Chat panel - Set to 'true' to enable (disabled by default for MVP)
NEXT_PUBLIC_ENABLE_AI_CHAT=false

# Trial Duration in milliseconds
# Dev: 5 minutes (300000), Prod: 2 weeks (1209600000)
TRIAL_DURATION_MS=1209600000

# Stripe Configuration
# Get these from your Stripe Dashboard: https://dashboard.stripe.com/

# Stripe API Keys
STRIPE_PUBLISHABLE_KEY=pk_test_...
STRIPE_SECRET_KEY=sk_test_...

# Webhook Secret (get from Stripe CLI or webhook settings)
STRIPE_WEBHOOK_SECRET=whsec_...

# Product Price IDs (get these after creating products in Stripe)
# Pro Plan Price IDs
STRIPE_PRICE_PRO_MONTHLY=price_...
STRIPE_PRICE_PRO_YEARLY=price_...

# Enterprise Plan Price IDs
STRIPE_PRICE_ENTERPRISE_MONTHLY=price_...
STRIPE_PRICE_ENTERPRISE_YEARLY=price_...

# Optional: Customer Portal Configuration
STRIPE_CUSTOMER_PORTAL_URL=https://billing.stripe.com/p/login/...

# For Next.js public access (duplicate the publishable key)
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_test_...
