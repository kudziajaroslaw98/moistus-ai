# E2E Testing Environment Variables
# Copy this to .env.e2e.local and fill in the values

# Supabase connection
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=<your-anon-key>

# Service role key - required for screenshot sync to storage
SUPABASE_SERVICE_ROLE_KEY=<your-service-role-key>

# Test user credentials
TEST_USER_EMAIL=e2e-test@example.com
TEST_USER_PASSWORD=test-password-123

# Optional: Use a specific map ID instead of creating new ones
TEST_MAP_ID=<your-test-map-uuid>

# Bypass limits for testing
NEXT_PUBLIC_DEV_BYPASS_LIMITS=true

# Trial Duration - short for E2E testing (5 minutes)
TRIAL_DURATION_MS=300000
